        "cordova-sqlite-storage": "^2.5.1",
        "cordova-support-google-services": "^1.1.0",
        "firebase": "^5.5.8",
        "hammerjs": "2.0.8",
        "ionic-angular": "3.9.2",
        "ionic-plugin-keyboard": "2.2.1",
