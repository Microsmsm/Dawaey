        "node-sass": "^4.10.0",
        "phonegap-plugin-multidex": "^1.0.0",
        "phonegap-plugin-push": "^2.2.3",
        "promise-polyfill": "^8.1.0",
        "rxjs": "^6.3.3",
        "rxjs-compat": "6.3.3",
        "sw-toolbox": "3.6.0",
        "zone.js": "0.8.20"
    },
    "devDependencies": {
        "@ionic/app-scripts": "^3.2.0",
